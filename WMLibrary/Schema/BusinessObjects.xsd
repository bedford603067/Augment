        </xs:annotation>
      </xs:element>
      <xs:element name="ValidationMessage" type="xs:string">
        <xs:annotation>
          <xs:documentation>User friendly message to post when Regex expression not matched</xs:documentation>
        </xs:annotation>
      </xs:element>
