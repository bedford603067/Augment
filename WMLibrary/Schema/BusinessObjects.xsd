<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="BusinessObjects" targetNamespace="http://FinalBuild.co.uk/BusinessObjects.WorkManagement" elementFormDefault="qualified" xmlns="http://FinalBuild.co.uk/BusinessObjects.WorkManagement" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<!-- Elements (for XmlTypeExport purposes)  -->
	<xs:element name="Job" type="Job" />
	<xs:element name="JobUpdates" type="JobUpdateCollection" />
	<xs:element name="JobUpdate" type="JobUpdate" />
	<xs:element name="ReferenceTypes" type="ReferenceTypeCollection" />
	<xs:element name="EngineerFeedback" type="EngineerFeedback" />
	<xs:element name="JobReferences" type="JobReferenceCollection" />
	<xs:element name="JobStatusChange" type="JobStatusChange" />
	<xs:element name="LookupContainer" type="LookupContainer" />
	<xs:element name="Customer" type="Customer" />
	<xs:element name="ChangeRequests" type="ChangeRequestCollection" />
	<xs:element name="Activities" type="ActivityCollection" />
	<xs:element name="LibraryTasks" type="ActivityTaskTemplateCollection" />
	<xs:element name="JobRequests" type="JobRequestCollection" />
	<xs:element name="TaskLookupData" type="TaskLookupData" />
	<xs:element name="Priorities" type="PriorityLookupData" />
	<xs:element name="AuditRecord" type="WorkflowAuditRecord" />
	<xs:element name="JobFailures" type="JobFailureCollection" />
	<xs:element name="LostTimeReasons" type="LostTimeReasonCollection" />
	<xs:element name="IncompleteReasons" type="IncompleteReasonCollection" />
	<xs:element name="BankHolidays" type="BankHolidayCollection" />
	<xs:element name="CoreLookupData" type="CoreLookupData" />
	<xs:element name="WorkContainer" type="WorkContainer" />
	<xs:element name="EngineerNonAvailabilities" type="EngineerNonAvailabilityCollection" />
	<xs:element name="AssignmentDetailsCollection" type="AssignmentDetailsCollection" />
	<xs:element name="AssetLookupData" type="AssetLookupData" />
	<xs:element name="GPSInformation" type="GPSInformation" />
	<xs:element name="MeterBase" type="MeterBase" />
	<xs:element name="StatusConflictCollection" type="StatusConflictCollection" />
	<xs:element name="ExtendedReferenceTypes" type="ExtendedReferenceTypeCollection" />
	<xs:element name="PerformanceMeasurementGroups" type="PerformanceMeasurementGroupCollection" />
	<xs:element name="SearchParameters" type="SearchParameterCollection" />
	<xs:element name="WorkSchedulingContainer" type="WorkSchedulingContainer" />
	<xs:element name="ScheduleDetailsCollection" type="ScheduleDetailsCollection" />
	<xs:element name="EngineerSkills" type="EngineerSkillCollection" />
	<xs:element name="EngineerStandbyCollection" type="EngineerStandbyCollection" />
	<xs:element name="RecurringNonAvailabilityCollection" type="RecurringNonAvailabilityCollection" />
	<xs:element name="WorkingWeek" type="WorkingWeek" />
	<xs:element name="ExtendedAssignment" type="ExtendedAssignment" />
  <xs:element name="SupportingAssignmentDetailsCollection" type="SupportingAssignmentDetailsCollection" />
  <xs:element name="Stock" type="Stock" />
  <xs:element name="StockItemCollection" type="StockItemCollection" />
  <xs:element name="StockItem" type="StockItem" />
  <xs:element name="StockLocation" type="StockLocation" />
  <xs:element name="StockTaskUpdate" type="StockTaskUpdate"/>
  <xs:element name="StockTaskUpdates" type="StockTaskUpdateCollection"/>
  <xs:element name="AssignmentConstraints" type="AssignmentConstraints"/>
  <xs:element name="OptimisationRequest" type="OptimisationRequest"/>
  
  <!-- Complex Types -->
	<!-- Base -->
	<xs:complexType name="Job" abstract="true">
		<xs:sequence>
			<xs:element name="ID" type="xs:int">
				<xs:annotation>
					<xs:documentation>Unique identifier for the entity within the Work Management system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceSystem" type="eWMSourceSystem">
				<xs:annotation>
					<xs:documentation>Business Stream</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkType" type="eWorkType" default="Reactive">
				<xs:annotation>
					<xs:documentation>Work Type as defined by the Enumeration, e.g., Reactive</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="eJobStatus">
				<xs:annotation>
					<xs:documentation>Status as defined by the Enumeration, e.g., Dispatched</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DueDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>The current Date\Time by which the Job must be completed. Dynamic through lifetime of a Job. Assignment.Start in Click terms.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlannedDueDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>The initially planned Date\Time by which the Job must be completed. Static through lifetime of a Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Location" type="Location">
				<xs:annotation>
					<xs:documentation>The Location where the work is to be carried out</xs:documentation>
					<xs:documentation>One of the types {Location, Site, Property}</xs:documentation>
					<xs:documentation>For Jobs involving a Start and End location, e..g, Sludge, this attribute represents Starting Location</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Asset" type="Asset">
				<xs:annotation>
					<xs:documentation>The Asset the work is to be carried out in respect of</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsComplete" type="xs:boolean" minOccurs="1" default="false">
				<xs:annotation>
					<xs:documentation>This flag signals the Field worker is finished with the Job. When true the Status should be one of Completed or Cancelled</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateCreated" type="xs:dateTime" minOccurs="1">
				<xs:annotation>
					<xs:documentation>The Date the Job was created in the Work Management system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SchedulingPriority" type="SchedulingPriority" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Scheduling (Lead Time) Priority value for the Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DispatchPriority" type="DispatchPriority" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Dispatch Priority (Click) value for the Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastUser" type="xs:string">
				<xs:annotation>
					<xs:documentation>The Last User of the Work Management system to amend the Job</xs:documentation>
					<xs:documentation>XmlIgnore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastUpdated" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>The Date the Job was last updated in the Work Management system to amend the Job.</xs:documentation>
					<xs:documentation>XmlIgnore</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Optional attributes (Not required to create a Job instance) -->
			<xs:element name="Description" type="xs:string">
				<xs:annotation>
					<xs:documentation>This attribute is reserved for a Summary description of the Job which may vary according to Business Stream, Work Type, Asset, etc</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RaisedBy" type="xs:string">
				<xs:annotation>
					<xs:documentation>User who created the Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Originator" type="xs:string">
				<xs:annotation>
					<xs:documentation>User on whose behalf the Job was created - usually the same as RaisedBy</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Workers" type="WorkerCollection">
				<xs:annotation>
					<xs:documentation>Engineer(s) the Job is assigned to</xs:documentation>
					<xs:documentation>Note. Crew is not explicitly defined at present as is not per se relevant to Field delivery</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Appointment" type="Appointment">
				<xs:annotation>
					<xs:documentation>Appointment associated with the Job (where applicable)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FileAssociations" type="FileAssociationCollection">
				<xs:annotation>
					<xs:documentation>Pointers to Files associated with the Job, e.g., Utility Plans</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartDateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Time recorded for the Job starting (in the Field)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndDateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Time recorded for the Job ending (in the Field)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AreaDetails" type="AreaDetails">
				<xs:annotation>
					<xs:documentation>Primary and SubArea(s) associated with the Job (Region and District in Click terms)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Tasks" type="ActivityTaskCollection">
				<xs:annotation>
					<xs:documentation>Child Tasks of the Job. May be ActivityTask inheritors</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Activity" type="ActivityReference">
				<xs:annotation>
					<xs:documentation>This attribute is reserved for a Summary description of the Activity template the Job is based upon</xs:documentation>
					<xs:documentation>Note. Job.Tasks and Activity.Tasks need not be identical so Activity as class Activity is not appropriate</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EarliestStartDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>The earliest Date\Time that the Job should start (Scheduling Task.EarlyStart attribute)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Comments" type="CommentAuditRecordCollection">
				<xs:annotation>
					<xs:documentation>Comments (history) associated with the Job.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentJobReference" type="AssociatedJobReference">
				<xs:annotation>
					<xs:documentation>(Parent) Linked Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActivityDuration" type="xs:int">
				<xs:annotation>
					<xs:documentation>Duration for completing the Task in Minutes. Duration itself is derived from Tasks or from this Activity.Duration value which may or may not be set at Job creation time</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateReopened" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>The Date the Job was reopened in the Work Management system (was previously Completed)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OnHoldReasons" type="OnHoldReasonCollection" nillable="true">
				<xs:annotation>
					<xs:documentation>On Hold Reasons</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RaisedInField" type="xs:boolean" minOccurs="1" default="false">
				<xs:annotation>
					<xs:documentation>Indicates if Job was created in response to a Field user Job Request (JobRequest.IsFieldUpdate) </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CostCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Cost Code associated with the Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncompleteOutcomeHistory" type="JobIncompleteOutcomeCollection">
				<xs:annotation>
					<xs:documentation>History of occurrences of the Job being stopped as Incomplete\Paused including the IncompleteReason selected</xs:documentation>
				</xs:annotation>
			</xs:element>
		  <xs:element name="SkillsBreakdown" type="SkillsMatrix">
			<xs:annotation>
			  <xs:documentation>The Skills per Resource required to carry out the Task</xs:documentation>
			</xs:annotation>
		  </xs:element>
			<xs:element name="RevisedDueDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>The Revised (from original PlannedDueDate) Date\Time by which the Job must be completed. Applies to all assignments\instances of the Job</xs:documentation>
				</xs:annotation>
			</xs:element>
      <xs:element name="Project" type="ReferenceType">
        <xs:annotation>
          <xs:documentation>Allow association to an overall Project</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Contact" type="Contact">
        <xs:annotation>
          <xs:documentation>Optional, to hold e.g Project Contact</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Customer" type="Customer">
        <xs:annotation>
          <xs:documentation>Optional, to hold the Job's Customer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AssignmentConstraints" type="AssignmentConstraints">
        <xs:annotation>
          <xs:documentation>Optional, to hold any contraints that should be considered when creating Assignments for this Job.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
	</xs:complexType>
	<xs:complexType name="LocationBase" abstract="true">
		<xs:annotation>
			<xs:documentation>This type is inherited by the Location and Property entities which include attributes for Street Address and Site respectively</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PostCode">
				<xs:annotation>
					<xs:documentation>UK PostCode http://www.govtalk.gov.uk/gdsc/html/frames/Postcode.htm</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
            <xs:pattern value="^([A-Z]{1,2}[0-9][0-9A-Z]{0,1} {0,1}[0-9][A-Z]{2})$" >
              <xs:annotation>
                <xs:documentation>'Post Code' is a mandatory field that must be supplied in a valid format e.g XX1 1XX.</xs:documentation>
              </xs:annotation>
            </xs:pattern>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="XCoord">
				<xs:annotation>
					<xs:documentation>GPS X coordinate EASTING, should be exactly 6 numeric characters</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:pattern value="([0-9]{6})">
            <xs:annotation>
					  <xs:documentation>GPS X coordinate EASTING, should be exactly 6 numeric characters</xs:documentation>
				    </xs:annotation>
           </xs:pattern>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="YCoord">
				<xs:annotation>
					<xs:documentation>GPS Y coordinate NORTHING, should be exactly 6 numeric characters</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
            <xs:pattern value="([0-9]{6})">
              <xs:annotation>
                <xs:documentation>GPS Y coordinate NORTHING, should be exactly 6 numeric characters</xs:documentation>
              </xs:annotation>
            </xs:pattern>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
      <xs:element name="Latitude" type="xs:double">
        <xs:annotation>
          <xs:documentation>WGS84 — SRID 4326 standard Latitude</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Longitude" type="xs:double">
        <xs:annotation>
          <xs:documentation>WGS84 — SRID 4326 standard Longitude</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
	</xs:complexType>
	<xs:complexType name="JobUpdateCollection">
		<xs:sequence>
			<xs:element name="JobUpdate" type="JobUpdate" maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JobUpdate" abstract="true">
		<xs:sequence>
			<xs:element name="ID" type="xs:int">
				<xs:annotation>
					<xs:documentation>ID of the Job being updated</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceSystem" type="eWMSourceSystem">
				<xs:annotation>
					<xs:documentation>Business Stream</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="eJobStatus">
				<xs:annotation>
					<xs:documentation>(New) Status of the Job being updated</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsComplete" type="xs:boolean" minOccurs="1" default="false">
				<xs:annotation>
					<xs:documentation>This flag signals the Field worker is finished with the Job. When true the Status should be one of Completed or Cancelled</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsLoneWorker" type="xs:boolean" minOccurs="1" default="false">
				<xs:annotation>
					<xs:documentation>Flag denoting whether the assigned Worker is currently logged on to the Lone Worker system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="xs:string">
				<xs:annotation>
					<xs:documentation>Description of the Update (for Field tracking purposes)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartDateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Start Date Time recorded for the Update (locally on Field device)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="1" maxOccurs="1" name="EndDateTime" nillable="true" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>End Date Time recorded for the Update (locally on Field device)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UserID" type="xs:string">
				<xs:annotation>
					<xs:documentation>The User who updated the Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsFieldUpdate" type="xs:boolean" minOccurs="1" default="true">
				<xs:annotation>
					<xs:documentation>This flag indicates Update is being submitted by a Field worker, not an Office worker using the Field client application</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WindowsIdentity" type="xs:string">
				<xs:annotation>
					<xs:documentation>The WindowsIdentity of the User who updated the Job e.g. when logged on as a Gang will be != UserID</xs:documentation>
				</xs:annotation>
			</xs:element>
      <xs:element name="SchedulerAlert" type="eSchedulerAlertType">
        <xs:annotation>
          <xs:documentation>When not default ("None") the JobUpdate should only be processed to create an Alert and nothing else</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
	</xs:complexType>
	<xs:complexType name="LookupContainer" abstract="true">
		<xs:annotation>
			<xs:documentation>This type supports the Field Management service and is solely for internal system use</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SourceSystem" type="eWMSourceSystem">
				<xs:annotation>
					<xs:documentation>Business Stream</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastUpdated" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Date\Time the LookupData contained was last updated</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
  <xs:complexType name="Contact">
		<xs:annotation>
			<xs:documentation>This type is inherited by Customer and by an entity within the BusinessObjects.CRM namespace</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Surname">
				<xs:annotation>
					<xs:documentation>Customer Surname within CRM master system, minimum of 2 alphabetical characters (maximum of 40, TBC)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="^([A-Za-z]{2,40})$" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Forenames" type="xs:string" />
			<xs:element name="TelephoneNo">
				<xs:annotation>
					<xs:documentation>UK Telephone No. http://www.ofcom.org.uk/telecoms/ioi/numbers/numbers_administered/</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
            <xs:pattern value="(((\+44)? ?(\(0\))? ?)|(0))( ?[0-9]{3,4}){3}">
              <xs:annotation>
                <xs:documentation>Telephone No. is not in a valid format</xs:documentation>
              </xs:annotation>
            </xs:pattern>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MobileNo">
				<xs:annotation>
					<xs:documentation>UK Telephone No. http://www.ofcom.org.uk/telecoms/ioi/numbers/numbers_administered/</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="(((\+44)? ?(\(0\))? ?)|(0))( ?[0-9]{3,4}){3}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EMail">
				<xs:annotation>
					<xs:documentation>Email Address/</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})$" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AlternativeTelephoneNo">
				<xs:annotation>
					<xs:documentation>UK Telephone No. http://www.ofcom.org.uk/telecoms/ioi/numbers/numbers_administered/</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="(((\+44)? ?(\(0\))? ?)|(0))( ?[0-9]{3,4}){3}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CompanyName" type="xs:string" />
			<xs:element name="Title" type="xs:string" />
      <xs:element name="Department" type="xs:string" />
      <xs:element name="Role" type="xs:string" />
      <xs:element name="Notes" type="xs:string" />
    </xs:sequence>
	</xs:complexType>
  <xs:complexType name="ContactCollection">
    <xs:sequence>
      <xs:element name="ContactCollection" type="Contact" maxOccurs="unbounded" minOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <!-- Core -->
  <xs:complexType name="Asset">
    <xs:sequence>
      <xs:element name="ID" type="xs:int">
        <xs:annotation>
          <xs:documentation>Unique identifier for the entity within the Asset Management system</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Description">
        <xs:annotation>
          <xs:documentation>This attribute is reserved for a Summary description of the Asset which may vary according to Asset, Site (if applicable), etc</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="^(?=.*\S).+$">
              <xs:annotation>
                <xs:documentation>'Description' is a mandatory field and has not been specified.</xs:documentation>
              </xs:annotation>
            </xs:pattern>
            <xs:minLength value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Code" type="xs:string">
        <xs:annotation>
          <xs:documentation>Alternate key for the entity within the Asset Management system that the User knows the entity by</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Location" type="Location">
        <!--
        <xs:annotation>
          <xs:documentation>This element will not be Serialized to Field devices as Location information will be present in the (parent) Job instance</xs:documentation>
          <xs:documentation>XmlIgnore</xs:documentation>
        </xs:annotation>
        -->
      </xs:element>
      <xs:element name="AssetType" type="xs:string">
        <xs:annotation>
          <xs:documentation>The Discipline of the Asset e.g Civil, Mechanical, etc</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Indicators" type="PerformanceIndicatorCollection">
        <xs:annotation>
          <xs:documentation>Directs Measurements to be gathered against the Asset</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IndicatorGroups" type="PerformanceIndicatorGroupCollection">
        <xs:annotation>
          <xs:documentation>Directs how Measurements are to be gathered in "sets" against the Asset</xs:documentation>
        </xs:annotation>
      </xs:element>
