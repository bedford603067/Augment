        <xs:annotation>
          <xs:documentation>This flag indicates to client software (AAA) if value can be edited or not</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MaxLength" type="xs:int">
        <xs:annotation>
          <xs:documentation>Max length of Value when it is natively a varchar in SQL Server</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DataType" type="xs:string">
        <xs:annotation>
          <xs:documentation>The native DataType of Value</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Position" type="xs:int">
        <xs:annotation>
          <xs:documentation>Allows control of the Display Order that Attributes appear in</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IsMandatory" type="xs:boolean" minOccurs="1" default="false">
        <xs:annotation>
          <xs:documentation>This flag indicates to client software (AAA) if value required on a New asset</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="IsHidden" type="xs:boolean" minOccurs="1" default="false">
        <xs:annotation>
          <xs:documentation>This flag indicates to client software (AAA) if value should be displayed to User</xs:documentation>
        </xs:annotation>
