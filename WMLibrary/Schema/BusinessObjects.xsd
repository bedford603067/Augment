  <xs:complexType name="Worker">
		<xs:sequence>
      <xs:element name="UserID" type="xs:string" />      
			<xs:element name="LoginName">
				<xs:annotation>
					<xs:documentation>AD login, maximum of 7 characters (minimum of 3, TBC)</xs:documentation>
					<xs:documentation>The composite of LoginName and EmpNo is the unique identifier for the Employee within the Scheduling system (Click)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="^([A-Za-z]{3,7})$" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EmpNo">
				<xs:annotation>
					<xs:documentation>Employee No within HR master system, maximum of 10 all numeric characters (minimum of 6, TBC)</xs:documentation>
					<xs:documentation>The composite of LoginName and EmpNo is the unique identifier for the Employee within the Scheduling system (Click)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="^([0-9]{6,10})$" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Surname">
				<xs:annotation>
					<xs:documentation>Employee Surname within HR master system, minimum of 2 alphabetical characters (maximum of 40, TBC)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="^([A-Za-z]{2,40})$" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Forenames" type="xs:string" />
			<xs:element name="EmpPrefname" type="xs:string" />
			<xs:element name="AreaDetails" type="AreaDetails">
				<xs:annotation>
					<xs:documentation>Primary and SubArea(s) associated with the Job (Region and District in Click terms)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Skills" type="SkillCollection">
				<xs:annotation>
					<xs:documentation>Skills in respect of suitability to carry out a Job</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmpAddress" type="xs:string" />
			<xs:element name="Email">
				<xs:annotation>
					<xs:documentation>Email Address/</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})$" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MobileNo">
				<xs:annotation>
					<xs:documentation>UK Telephone No. http://www.ofcom.org.uk/telecoms/ioi/numbers/numbers_administered/</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="(((\+44)? ?(\(0\))? ?)|(0))( ?[0-9]{3,4}){3}" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
      <xs:element name="SequenceNo" type="xs:int">
        <xs:annotation>
          <xs:documentation>Used to order Workers within WorkerCollection, e.g Gang Members</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WorkingWeek" type="WorkingWeek">
